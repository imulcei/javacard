<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Supprimer?" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="deleteYesButton" mnemonicParsing="false" onAction="#confirmDeleteContact" text="Oui" />
            <Button fx:id="deleteNoButton" mnemonicParsing="false" onAction="#cancelDeleteContact" text="Non" />
         </children>
      </HBox>
   </children>
</VBox>
